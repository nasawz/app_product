{
  "name": "category",
  "description": "商品类别",
  "base": "PersistedModel",
  "idInjection": true,
  "properties": {
    "name": {
      "type": "string",
      "description": "类别名称"
    },
    "parentId": {
      "type": "string"
    },
    "parentName": {
      "type": "string",
      "description": "父类别"
    },
    "sort": {
      "type": "number",
      "description": "排序值"
    },
    "status": {
      "type": "string"
    },
    "created": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "event": {
      "type": "belongsTo",
      "model": "event",
      "foreignKey": "eventId"
    }
  },
  "acls": [],
  "methods": []
}
